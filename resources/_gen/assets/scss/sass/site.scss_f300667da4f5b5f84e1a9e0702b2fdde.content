html,
body {
  margin: 0;
  height: 100%;
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol"; }

body {
  display: flex;
  flex-direction: column;
  flex-basis: 70px;
  font-size: 16px;
  background: #021526; }

header {
  background: #021526;
  color: #fff;
  padding-top: 100px; }
  header:after {
    content: '';
    display: block;
    height: 4px;
    width: 100%;
    margin-top: 2rem; }
  header:after {
    content: '';
    display: block;
    height: 4px;
    width: 100%;
    background: #00F260;
    background: -webkit-linear-gradient(to left, #095B8D, #00F260);
    background: linear-gradient(to left, #095B8D, #00F260); }
  header h1 {
    font-size: 2rem;
    line-height: 1.2;
    transition: padding 1s ease; }
    @media screen and (min-width: 500px) {
      header h1 {
        font-size: 50px; } }

.triangle-1,
.triangle-2 {
  position: absolute;
  box-shadow: -2px -2px 5px #0002; }

.triangle-1 {
  width: 500px;
  top: 0;
  right: 0;
  opacity: 1;
  margin-right: -500px;
  background: #031D35;
  transform-origin: top left;
  z-index: 310; }

.triangle-2 {
  top: 0;
  left: 0;
  width: 2000px;
  height: 1000px;
  margin-top: -1000px;
  z-index: 300;
  background: #021526;
  transform-origin: bottom left; }

.homepage-content {
  background: #042543;
  height: calc(100vh);
  max-height: 100%;
  padding: 50px;
  width: 100%;
  max-width: 100%;
  overflow: hidden; }
  .homepage-content article {
    position: relative;
    z-index: 500;
    color: white; }

.homepage main {
  overflow: hidden; }

main {
  width: 100%;
  position: relative;
  font-size: 16px;
  background-color: #F6F7F9; }
  main > header {
    display: flex; }
    @media screen and (min-width: 960px) {
      main > header {
        flex-direction: column; } }

article > header,
section > header {
  transition: padding 1s ease; }
  article > header h1,
  section > header h1 {
    color: inherit;
    -webkit-font-smoothing: antialiased;
    font-weight: bold;
    font-size: 2.2rem; }
    @media screen and (min-width: 500px) {
      article > header h1,
      section > header h1 {
        font-size: 3rem; } }

article.content {
  font-size: 20px;
  color: #303538;
  line-height: 32px;
  font-family: "Georgia", serif;
  padding: 2rem; }
  article.content > *:first-child {
    padding-top: 0;
    margin-top: 0; }
  article.content > *:last-child {
    padding-bottom: 0;
    margin-bottom: 0; }

.container {
  max-width: 1440px;
  transition: width 1s ease;
  margin: 0 auto; }

.highlight {
  width: 100%;
  transition: all .7s ease;
  position: relative;
  overflow: hidden;
  line-height: 1.1em; }
  .highlight pre {
    padding: 1rem;
    margin-top: 0;
    border-radius: 5px;
    background-color: #1B1917 !important;
    transition: all .7s ease; }
    @media screen and (min-width: 500px) {
      .highlight pre {
        padding: 2rem; } }

img {
  max-width: 100%;
  height: auto; }

.post-content {
  font-size: 20px;
  color: inherit;
  line-height: 32px;
  margin-bottom: 1.5em;
  font-family: "Georgia", serif;
  max-width: 960px; }
  .post-content p {
    margin-bottom: 1.5em; }
  .post-content > :first-child {
    margin-top: 0;
    padding-top: 0; }
  .post-content > :last-child {
    margin-bottom: 0;
    padding-bottom: 0; }
  .post-content a {
    color: #063c5d;
